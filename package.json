{
  "name": "backend-akagera",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d dist",
    "start": "node -r @babel/register src/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seed",
    "migrate:down": "sequelize db:migrate:undo:all",
    "migrate:create": "npx sequelize-cli model:generate --name User --attributes firstName:string,lastName:string,email:string"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "async-handler": "^0.0.3",
    "body-parser": "^1.20.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.3",
    "path": "^0.12.7",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "uuid4": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/node": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/register": "^7.24.6",
    "nodemon": "^3.1.4",
    "sequelize-cli": "^6.6.2"
  }
}
