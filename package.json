{
  "name": "backend-akagera",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d dist",
    "start": "node -r @babel/register src/index.js",
    "dev": "nodemon --exec babel-node src/index.js",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "seed:down": "sequelize db:seed:undo:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seed",
    "migrate:down": "sequelize db:migrate:undo:all",
    "setup":"npm run migrate && npm run seed",
    "migrate:create": "npx sequelize-cli model:generate --name NextedSubCategory --attributes firstName:string,lastName:string,email:string"
  },
